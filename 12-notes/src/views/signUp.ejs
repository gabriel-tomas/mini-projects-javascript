<%- include("includes/head") %>
    <div class="space-all text-center gap-top-4">
        <h1 class="font-size-2">Se cadastre para ver suas Notas</h1>
    </div>
    <div class="space-all alert">
        <span>
            <% if(exist) { %>
                <%= exist %>
            <% } %>
        </span>
    </div>
    <section class="container signUp space-all">
        <form class="form gap-top-2-5" method="post">
            <input type="hidden" name="_csrf" value="<%= csrfToken %>">
            <div class="gap-bottom0">
                <label class="gap-bottom-05 font-size--1 font-300" for="first-name">Primeiro nome:</label>
                <input class="secondary-input text-color back-neutral-0" type="text" name="firstName" id="first-name" autocomplete="given-name">
            </div>
            <div class="gap-bottom0">
                <label class="gap-bottom-05 font-size--1 font-300" for="last-name">Sobrenome:</label>
                <input class="secondary-input text-color back-neutral-0" type="text" name="lastName" id="last-name" autocomplete="family-name">
            </div>
            <div class="gap-bottom0">
                <label class="gap-bottom-05 font-size--1 font-300" for="user-name">Nome de usu√°rio:</label>
                <input class="secondary-input text-color back-neutral-0" type="text" name="userName" id="user-name" autocomplete="nickname">
            </div>
            <div class="gap-bottom0">
                <label class="gap-bottom-05 font-size--1 font-300" for="password">Senha:</label>
                <input class="secondary-input text-color back-neutral-0" type="password" name="password" id="password" autocomplete="current-password">
            </div>
            <div class="gap-top-1">
                <button class="button back-accent font-size--1 font-700" type="submit">Cadastrar</button>
            </div>
        </form>
    </section>
<%- include("includes/footer") %>